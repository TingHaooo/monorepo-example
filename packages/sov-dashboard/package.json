{
  "name": "sov-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "server": "yarn build && ts-node-dev --project ../sov-api/tsconfig.json ./src/server.ts",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:docker:up": "docker-compose up",
    "dev:docker:down": "docker-compose down",
    "dev:docker:install": "docker-compose build",
    "dev:docker:uninstall": "docker-compose down --rmi all",
    "prod:docker:up": "docker-compose -f docker-compose.prod.yml up",
    "prod:docker:down": "docker-compose -f docker-compose.prod.yml down",
    "prod:docker:install": "docker-compose -f docker-compose.prod.yml build",
    "prod:docker:uninstall": "docker-compose -f docker-compose.prod.yml down --rmi all"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}