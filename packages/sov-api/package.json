{
  "name": "sov-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "dev": "ts-node-dev ./src/server.ts",
    "dev:docker:up": "docker-compose up",
    "dev:docker:down": "docker-compose down",
    "dev:docker:install": "docker-compose build",
    "dev:docker:uninstall": "docker-compose down --rmi all",
    "prod:docker:up": "docker-compose -f docker-compose.prod.yml up",
    "prod:docker:down": "docker-compose -f docker-compose.prod.yml down",
    "prod:docker:install": "docker-compose -f docker-compose.prod.yml build",
    "prod:docker:uninstall": "docker-compose -f docker-compose.prod.yml down --rmi all"
  },
  "dependencies": {
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.14",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/redis": "^2.8.22",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}